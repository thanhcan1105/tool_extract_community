                    "controller.render.laser": "v.laser"
                },
                {
                    "controller.render.sight": "v.sight"
                },
                {
                    "controller.render.silencer": "v.silencer"
                },
                {
                    "controller.render.m67_grenade": "v.m67_grenade"
                },
                {
                    "controller.render.nv_goggles": "q.self->q.is_item_name_any('slot.armor.head', 0, 'gabrielaplok:nv_goggles') && v.is_first_person && !q.is_in_water"
                },
                {
                    "controller.render.sight_ui": "v.sight_ui && q.is_sneaking && v.is_first_person"
                }
            ],
            "enable_attachables": true
        }
    }
}