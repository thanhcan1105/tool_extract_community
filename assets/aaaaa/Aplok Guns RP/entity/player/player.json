            "scripts": {
                "scale": "0.9375",
                "initialize": [
                    "v.is_holding_right = 0;",
                    "v.is_blinking = 0;",
                    "v.last_blink_time = 0;",
                    "v.hand_bob = 0;",
                    "v.is_standing = 0;",
                    "v.is_inspecting = 0;"
                ],
                "pre_animation": [
                    "v.helmet_layer_visible = !q.self->q.is_item_name_any('slot.armor.head', 0, 'minecraft:air');",
                    "v.leg_layer_visible = !q.self->q.is_item_name_any('slot.armor.chest', 0, 'minecraft:air');",
                    "v.boot_layer_visible = !q.self->q.is_item_name_any('slot.armor.legs', 0, 'minecraft:air');",
                    "v.chest_layer_visible = !q.self->q.is_item_name_any('slot.armor.feet', 0, 'minecraft:air');",
                    "v.attack_body_rot_y = math.sin(360 * math.sqrt(v.attack_time)) * 5;",
                    "v.tcos0 = (math.cos(q.modified_distance_moved * 38.17) * q.modified_move_speed / v.gliding_speed_value) * 57.3;",
                    "v.first_person_rotation_factor = math.sin((1 - v.attack_time) * 180);",
                    "v.hand_bob = q.life_time < 0.01 ? 0 : v.hand_bob + ((q.is_on_ground && q.is_alive ? math.clamp(math.sqrt(math.pow(q.position_delta(0), 2) + math.pow(q.position_delta(2), 2)), 0, 0.1) : 0) - v.hand_bob) * 0.02;",
                    "v.map_angle = math.clamp(1 - v.player_x_rotation / 45.1, 0, 1);",
                    "v.item_use_normalized = q.main_hand_item_use_duration / q.main_hand_item_max_duration;",
                    "v.is_third_person = !v.is_first_person && !v.map_face_icon;",
                    "v.inspection = !v.attack_time && !q.main_hand_item_use_duration && !q.is_sneaking && !q.is_sprinting && !q.self->q.property('gabrielaplok:reloading') && q.is_alive;",
                    "v.standing = !v.is_inspecting && !q.main_hand_item_use_duration && !q.is_sneaking && !q.is_sprinting && !q.self->q.property('gabrielaplok:reloading') && q.is_alive;",
                    "v.ak47 = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:ak47');",
                    "v.awp = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:awp');",
                    "v.desert_eagle = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:desert_eagle');",
                    "v.fnfal = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:fnfal');",
                    "v.glock17 = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:glock17');",
                    "v.glock18 = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:glock18');",
                    "v.m4a1 = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:m4a1');",
                    "v.mossberg = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:mossberg');",
                    "v.mp5a5 = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:mp5a5');",
                    "v.m67_grenade = q.self->q.is_item_name_any('slot.weapon.mainhand', 'gabrielaplok:m67_grenade');",
                    "v.guns = q.equipped_item_all_tags('slot.weapon.mainhand', 'is_gun');",
                    "v.grenade = q.equipped_item_all_tags('slot.weapon.mainhand', 'is_grenade');",
                    "v.grip = (q.equipped_item_all_tags('slot.weapon.mainhand', 'allow_grip') ? (q.self->q.is_item_name_any('slot.inventory', 3, 'gabrielaplok:foregrip') ? 1 : 0) : 0);",
                    "v.laser = (q.equipped_item_all_tags('slot.weapon.mainhand', 'allow_laser') ? (q.self->q.is_item_name_any('slot.inventory', 1, 'gabrielaplok:laser') ? 1 : 0) : 0);",
                    "v.sight = (q.equipped_item_all_tags('slot.weapon.mainhand', 'allow_sight') ? (q.self->q.is_item_name_any('slot.inventory', 0, 'gabrielaplok:eotech_sight') ? 1 : (q.self->q.is_item_name_any('slot.inventory', 0, 'gabrielaplok:acog_sight') ? 2 : (q.self->q.is_item_name_any('slot.inventory', 0, 'gabrielaplok:scope_hp') ? 3 : (q.self->q.is_item_name_any('slot.inventory', 0, 'gabrielaplok:scope_lp') ? 4 : 0)))) : 0);",
                    "v.silencer = (q.equipped_item_all_tags('slot.weapon.mainhand', 'allow_silencer') ? (q.self->q.is_item_name_any('slot.inventory', 2, 'gabrielaplok:silencer') ? 1 : 0) : 0);",
                    "v.sight_ui = v.sight && v.sight != 1 && !q.self->q.property('gabrielaplok:reloading');",
                    "v.is_reloading = q.self->q.property('gabrielaplok:reloading');"
                ],
